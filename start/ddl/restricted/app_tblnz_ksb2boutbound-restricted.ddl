CREATE VIEW ${var:view_db}.app_tblnz_ksb2boutbound AS
SELECT z.fi64KSB2BKey, z.fi64BinaryDataKey, z.fstrBatchID, z.fstrB2BType, z.fstrB2BMessageType, z.fstrRecordID, z.fstrAPI, z.flngSchemeAccountKey, z.fstrSchemeIRDNumber, z.fstrSchemeID, z.fstrTradingPartner, z.flngMemberAccountKey, z.flngMemberCustomerKey, z.fstrMemberIRDNumber, z.fi64SourceKey, z.fstrExternalID, z.fcurAmount, z.fstrManuallyAddedBy, z.fdtmQueued, z.fdtmProcess, z.fstrDeletedBy, z.fdtmDeleted, z.fblnManual, z.fblnFileCreated, z.fi64KSB2BLinkKey, z.fstrWho, z.fdtmWhen, z.record_effective_timestamp, z.record_expiry_timestamp, z.record_active_flag, z.record_deleted_flag, z.insert_object_run_key, z.update_object_run_key FROM ${var:source_db}.app_tblnz_ksb2boutbound z LEFT OUTER JOIN ${var:source_db}.app_tblcustomerlevel cl ON z.flngMemberCustomerKey = cl.flngCustomerKey WHERE (nvl(cl.fstrCustomerLevel, 'STD') = 'STD' AND nvl(cl.record_active_flag, 'Y') = 'Y' AND nvl(cl.record_deleted_flag, 'N') = 'N');
