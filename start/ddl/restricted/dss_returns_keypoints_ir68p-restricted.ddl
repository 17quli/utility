CREATE VIEW ${var:view_db}.dss_returns_keypoints_ir68p AS
SELECT z.IRD_NUMBER, z.LOCATION_NUMBER, z.RETURN_PERIOD_DATE, z.RETURN_TYPE, z.RETURN_VERSION_NO, z.RETURN_STATUS, z.FORM_VERSION_NO, z.NUMBER_OF_ERROR_ITEMS, z.`TIMESTAMP`, z.CALC_EARNER_PREMIUM_424, z.CALC_LIABLE_EARNINGS_100074, z.CALC_LIABLE_EARNINGS_IR12_416, z.CALC_TAX_DEDS_100405, z.CERTS_FILED_BY_TAPE_DISK_303, z.CERT_TYPE_100406, z.DATE_CERTIFICATES_KEYED_814, z.EARNINGS_IR12_119, z.EARNINGS_IR13_120, z.HALEX_EARNER_PREMIUM_100497, z.GR_EARNINGS_TAX_DED_822, z.HALEX_NO_OF_IR12_ATT_1107, z.HALEX_NO_OF_IR13_ATT_1109, z.IR12_DN_FSTC_821, z.IR12_IR13_GROSS_EARNINGS_820, z.IR12_IR13_TOTAL_TAX_DEDS_819, z.IR12_TOTAL_DEDUCTIONS_121, z.LIABLE_EARNINGS_IR12_417, z.NO_OF_IR12_CERTS_ATT_1108, z.NO_OF_IR13_CERTS_ATT_1110, z.NUMBER_LIABLE_CERTS_415, z.OTHER_TAX_DEDUCTIONS_801, z.OVER_UNDER_PAYMENT_127, z.PREVIOUS_ISSUE_DATE_406, z.PREVIOUS_NOTICE_TYPE_405, z.PRE_ENCRIPTION_CHECK_100018, z.PRE_ENCRIPTION_IRD_NO_100019, z.PRE_ENCRIPTION_LOC_NO_100020, z.REFUND_TRANS_TO_ACC_125, z.RULE_49_FLAG_128, z.STD_DISCLAIMER_APPLIES_100045, z.TAX_FREE_ALLOWANCES_100404, z.TOTAL_EARNER_PREMIUM_123, z.TOTAL_FSTC_601, z.TOTAL_GROSS_100403, z.TOTAL_GROSS_EARNINGS_823, z.TOTAL_PAYE_DEDUCTIONS_122, z.record_effective_timestamp, z.record_expiry_timestamp, z.record_active_flag, z.record_deleted_flag, z.insert_object_run_key, z.update_object_run_key FROM ${var:source_db}.dss_returns_keypoints_ir68p z LEFT OUTER JOIN ${var:source_db}.dss_special_clients_all sc ON CAST(z.IRD_NUMBER AS INT) = CAST(sc.IRD_NUMBER AS INT) WHERE nvl(sc.special_classification, 'STD') = 'STD' AND nvl(sc.record_active_flag, 'Y') = 'Y' AND nvl(sc.record_deleted_flag, 'N') = 'N';
