CREATE VIEW ${var:view_db}.wrq_tblwebnoticeemail AS
SELECT z.flngWebNoticeKey, z.flngEmailKey, z.flngWebLogonKey, z.fstrEmail, z.fstrWho, z.fdtmWhen, z.record_effective_timestamp, z.record_expiry_timestamp, z.record_active_flag, z.record_deleted_flag, z.insert_object_run_key, z.update_object_run_key FROM ${var:source_db}.wrq_tblwebnoticeemail z LEFT OUTER JOIN (SELECT wl.flngWebLogonKey, cl.fstrcustomerlevel FROM ${var:source_db}.wrq_tblweblogon wl INNER JOIN ${var:source_db}.app_tblcustomerlevel cl ON wl.flngCustomerKey = cl.flngCustomerKey WHERE nvl(wl.record_active_flag, 'Y') = 'Y' AND nvl(wl.record_deleted_flag, 'N') = 'N' AND nvl(cl.record_active_flag, 'Y') = 'Y' AND nvl(cl.record_deleted_flag, 'N') = 'N') wlcl ON z.flngWebLogonKey = wlcl.flngWebLogonKey WHERE nvl(wlcl.fstrcustomerlevel, 'STD') = 'STD';
