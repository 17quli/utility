CREATE VIEW ${var:view_db}.app_tblnz_casfdbk AS
SELECT z.flngDocKey, z.fdtmReceived, z.fstrRaisedBy, z.fstrCallerType, z.fstrLodgementMethod, z.fstrRelatedTo, z.fstrBriefDescrip, z.fstrDetailsFdbk, z.fstrActions, z.fstrResolution, z.fstrLearningIdentified, z.fstrDetailsLearning, z.fblnUpheld, z.fblnNotUpheld, z.fblnPartialUpheld, z.fstrName, z.fstrEmail, z.fstrAddress, z.fstrPhone, z.fstrWho, z.fdtmWhen, z.record_effective_timestamp, z.record_expiry_timestamp, z.record_active_flag, z.record_deleted_flag, z.insert_object_run_key, z.update_object_run_key FROM ${var:source_db}.app_tblnz_casfdbk z LEFT OUTER JOIN ${var:source_db}.app_tblacaseall ca ON CAST(z.flngDocKey AS INT) = CAST(ca.flngDocKey AS INT) LEFT OUTER JOIN ${var:source_db}.app_tblcustomerlevel cl ON ca.flngcustomerkey = cl.flngcustomerkey WHERE nvl(ca.record_active_flag, 'Y') = 'Y' AND nvl(ca.record_deleted_flag, 'N') = 'N' AND nvl(cl.fstrCustomerLevel, 'STD') = 'STD' AND nvl(cl.record_active_flag, 'Y') = 'Y' AND nvl(cl.record_deleted_flag, 'N') = 'N';
