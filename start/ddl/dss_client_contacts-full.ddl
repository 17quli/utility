CREATE VIEW ${var:view_db}.dss_client_contacts AS
SELECT z.ird_number, z.location_number, z.tax_type, z.date_applied, z.contact_name, z.work_phone_number, z.phone_number, z.fax_number, z.cell_phone_number, z.date_ceased, z.`timestamp`, z.record_effective_timestamp, z.record_expiry_timestamp, z.record_active_flag, z.record_deleted_flag, z.insert_object_run_key, z.update_object_run_key FROM ${var:source_db}.dss_client_contacts z;
