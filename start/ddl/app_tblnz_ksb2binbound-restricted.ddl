CREATE VIEW ${var:view_db}.app_tblnz_ksb2binbound AS
SELECT z.fi64KSB2BKey, z.fi64WebServiceGatewayKey, z.fstrBatchID, z.fstrB2BType, z.fstrB2BMessageType, z.fstrRecordID, z.fstrAPI, z.flngSchemeAccountKey, z.fstrSchemeIRDNumber, z.fstrSchemeID, z.fstrTradingPartner, z.flngMemberAccountKey, z.flngMemberCustomerKey, z.fstrMemberIRDNumber, z.fi64SourceKey, z.flngWorkKey, z.fstrExternalID, z.fcurAmount, z.fstrStatus, z.fdtmProcess, z.fstrManuallyAddedBy, z.fdtmReceived, z.fdtmQueued, z.fdtmProcessing, z.fdtmCompleted, z.fdtmErrored, z.fstrDeletedBy, z.fdtmDeleted, z.fblnManual, z.fblnError, z.fblnInvalidSchema, z.fi64KSB2BLinkKey, z.fstrWho, z.fdtmWhen, z.record_effective_timestamp, z.record_expiry_timestamp, z.record_active_flag, z.record_deleted_flag, z.insert_object_run_key, z.update_object_run_key FROM ${var:source_db}.app_tblnz_ksb2binbound z LEFT OUTER JOIN ${var:source_db}.app_tblcustomerlevel cl ON z.flngMemberCustomerKey = cl.flngCustomerKey WHERE nvl(cl.record_active_flag, 'Y') = 'Y' AND nvl(cl.record_deleted_flag, 'N') = 'N' AND nvl(cl.fstrcustomerlevel, 'STD') = 'STD';
